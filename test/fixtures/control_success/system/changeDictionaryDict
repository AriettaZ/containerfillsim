FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          changePatchTypeDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dictionaryReplacement
{
    boundary
    {
     	"walls_.*"
    	{
        	type            wall;
   	}
	"outlet1_.*"
    	{
        	type            patch;
   	}
	"inlet1_.*"
    	{
        	type            patch;
   	}
    	"inlet2_.*"
    	{
        	type            patch;
	}
   }
}

