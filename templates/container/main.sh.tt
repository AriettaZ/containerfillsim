#PBS -N container
#PBS -l nodes=1:ppn=12
#PBS -l walltime=01:00:00
#PBS -j oe
#PBS -S /bin/bash

echo "---Job started at:"
date

echo "name = <%= @model.name %>"
echo "temperature = <%= @model.temperature %>"

echo "wall = <%= @model.wall.file_filename %>"

<% @model.inlets.each do |inlet| %>
  echo "inlet = <%= inlet.file_filename %>"
<% end %>

sleep 30

echo "---Job finished at:"
date
