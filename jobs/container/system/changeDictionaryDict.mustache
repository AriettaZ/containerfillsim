FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          changePatchTypeDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dictionaryReplacement
{
    boundary
    {
     	"{{walls_name}}_.*"
    	{
        	type            wall;
   	}
	{{#outlet_list}}
	"{{name}}_.*"
    	{
        	type            patch;
   	}
	{{/outlet_list}}
	{{#inlet_list}}
	"{{name}}_.*"
    	{
        	type            patch;
   	}
	{{/inlet_list}}
   }
}

